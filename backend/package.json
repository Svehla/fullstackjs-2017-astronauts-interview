{
  "name": "nodejs-cmd-build",
  "version": "1.0.0",
  "description": "",
  "main": "rovnatko.js",
  "author": "",
  "license": "ISC",
  "scripts": {
    "test": "mocha **/*.test.js --compilers js:babel-core/register",
    "test:watch": "mocha -w **/*.test.js --compilers js:babel-core/register",
    "eslint": "./node_modules/.bin/eslint ./src/index.js",
    "eslint:watch": "esw -w ./src/**/*.js",
    "eslint:fix": "./node_modules/.bin/eslint --fix **/*.js",
    "flow": "flow",
    "flow:watch": "flow-watch",
    "dev": "NODE_ENV=development nodemon ./src/index.js --exec babel-node --ignore './tmp/'",
    "dev-win": "nodemon ./src/index.js --exec babel-node --ignore './tmp/'",
    "clean": "rm -rf ./build && mkdir ./build ",
    "build": "babel src -d ./build/",
    "start-builded-script": "node build/index.js",
    "start-builded-script-pm2": "pm2 startup",
    "production": "npm run clean && npm run build && NODE_ENV=production npm run start-build-script",
    "production-win": "npm run clean && npm run build && npm run start-build-script"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.0.2",
    "eslint": "^4.0.0",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-watch": "^3.1.2",
    "flow": "^0.2.3",
    "flow-bin": "^0.48.0",
    "flow-watch": "^1.1.1",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "pm2": "^2.5.0"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "body-parser": "^1.17.2",
    "express": "^4.17.3",
    "postgraphql": "^3.3.0",
    "table-to-schema": "^0.1.3"
  }
}
